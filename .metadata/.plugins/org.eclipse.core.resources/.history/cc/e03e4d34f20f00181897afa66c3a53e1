package EjemplosArrays;

public class EjemplosArrays {

	public static void main(String[] args) {
			
		int[] arrayEnteros;
		arrayEnteros = new int[5];
		arrayEnteros[0] = 7;
		arrayEnteros[1] = 9;
		arrayEnteros[2] = 11;
		arrayEnteros[3] = 12;
		arrayEnteros[4] = 1;
		
	// Bucle for - inicialización; condición; iteración
		
		for( int i = 0; i< 5; i++) {
			System.out.println("array = " + arrayEnteros[i]);
		}
	}
}
